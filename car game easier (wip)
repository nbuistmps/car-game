command = ""
started = False
stopped = False
while True:
    command = input("> ").lower()
    if started == False:
        print ('started') 
        if started == True:
            print ("Car is already started")
    elif command == "stop":
        print ('stopped')
    elif command == "help":
        print ("""Start - starts your car
Stop - stops your car
Quit - quits the game """)
    elif command == "quit":
        break
    else:
        print ("I dont understand")
